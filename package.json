{
  "name": "qrcode-generator",
  "version": "1.0.0",
  "description": "专业的二维码生成器 - 支持自定义颜色、Logo嵌入、多格式下载",
  "author": "QR Code Generator Team",
  "license": "MIT",
  "homepage": "https://your-domain.com",
  "keywords": [
    "qrcode",
    "generator",
    "二维码",
    "logo",
    "custom-color",
    "offline"
  ],
  "features": {
    "colorCustomization": true,
    "logoEmbedding": true,
    "multipleFormats": true,
    "history": true,
    "responsive": true,
    "offline": true,
    "localStorage": true
  },
  "browsers": {
    "chrome": ">=90",
    "firefox": ">=88",
    "safari": ">=14",
    "edge": ">=90"
  },
  "deployment": {
    "type": "static",
    "servers": ["nginx", "apache", "github-pages", "vercel", "netlify"],
    "https": "recommended",
    "cdn": "optional"
  },
  "files": {
    "required": [
      "index.html",
      "style.css",
      "app.js",
      "qrcodegen.js"
    ],
    "optional": [
      ".htaccess",
      "nginx.conf",
      "deploy.sh"
    ]
  },
  "size": {
    "total": "~85KB",
    "html": "~10KB",
    "css": "~19KB",
    "js": "~42KB",
    "compressed": "~25KB"
  },
  "performance": {
    "gzip": true,
    "cache": true,
    "lazyLoad": true,
    "debounce": true
  },
  "security": {
    "xss": "protected",
    "clickjacking": "protected",
    "contentType": "protected",
    "https": "recommended"
  }
}
